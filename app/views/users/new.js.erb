<% if current_user.role == 'super_admin' %>
  var userContent = $(".users-container .stats");
  userContent.html("<%= j render 'users/form', organization: @organization %>");
<% else %>
  var userContent = $(".admin-container .user-content");
  userContent.html("<%= j render 'users/form', organization: @organization %>");
<% end %>
